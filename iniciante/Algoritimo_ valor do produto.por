programa {
  funcao inicio() {
    
  // Declaração de variaveis
  real v1, q1, valor_produto

  //entrada
  escreva("Digite o valor do produto: ")
  leia (v1)
  escreva ("Digite a quantidade: ")
  leia (q1)

  //processamento 
  valor_produto = (v1 * q1)

  //Saída
  escreva("Valor total do produto: ", valor_produto )
  }
}
